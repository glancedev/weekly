          "alwaysIncludePattern": false,
          "category": "info"
      },
      {
          "type": "dateFile",
          "filename": "log/error.log",
          "pattern": "-yyyy-MM-dd",
          "maxLogSize": 20480,
          "backups": 10,
          "alwaysIncludePattern": false,
          "category": "error"
      }
  ]
}